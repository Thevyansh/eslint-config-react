{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "object-curly-newline": "off",
    "no-unused-vars": 1,
    "quotes": [2, "single"],
    "no-use-before-define":"off"
  }
}
